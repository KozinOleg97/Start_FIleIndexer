<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
  <appSettings>
    
    <!--
      pathToOldFiles - путь до директории с не индексированными файлами
      pathToNewFiles - путь до дирректории с индексированными (переименованными) файлаим
      extentionPattern - ограничение для расширеня файлов (реализованно только одно расширение за раз) (* - любый файлы)
      pathToExlFile - путь до директории с отчётом excele
      excelFileName - имя файла отчёта excele 
      -->
    
    <add key="pathToOldFiles" value="E:\Programming\Start Testing Folder\"/>
    <add key="pathToNewFiles" value="E:\Programming\Start Testing Folder\New Files\"/>
    <add key="extentionPattern" value="*"/>
    <add key="pathToExlFile" value="E:\Programming\Start Testing Folder\Report\"/>
    <add key="excelFileName" value="Журнал техпроцессов"/>
    <add key="mainFontHight" value="11"/>
  </appSettings>
</configuration>
